<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="styleViewOrderText">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/colorBlack</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">right</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="styleTitleInStart">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:textColor">@color/colorSoftBlack</item>
        <item name="android:textSize">17sp</item>
        <item name="android:padding">10dp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="styleEditTextInStart">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:textColorHint">@color/colorGray</item>
        <item name="android:textSize">16sp</item>
        <item name="android:padding">10dp</item>
    </style>

    <style name="styleButtonInStart">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:textColor">@color/colorWhite</item>
        <item name="android:textSize">16sp</item>
        <item name="android:padding">10dp</item>
    </style>

    <style name="styleCalculatorButton">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/button_calculator</item>
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">19sp</item>
    </style>

    <style name="styleBookingListText">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/colorWhite</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="styleBookingListHeader">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/colorWhite</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="styleCustomerListHeader">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/colorSoftBlack</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="styleCustomerListItem">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/colorSoftBlack</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="styleBookingEntryEditText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">16sp</item>
        <item name="android:background">@color/colorGray</item>
        <item name="android:padding">10dp</item>
    </style>

    <style name="styleMainActionBarButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@drawable/button_action_bar</item>
        <item name="android:padding">10dp</item>
        <item name="android:layout_marginRight">15dp</item>
    </style>

    <style name="styleTextViewPrint">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">5dp</item>
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="styleTextViewHeaderPrint">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">5dp</item>
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="styleTextViewItemPrint">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">5dp</item>
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="styleBillPaymentLabel">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/colorWhite</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="styleBillListHeader">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/colorWhite</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="styleOkButton">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/button_transparent</item>
        <item name="android:textColor">@color/colorApp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:text">Ok</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="styleCancelButton">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/button_transparent</item>
        <item name="android:textColor">@color/colorSoftBlack</item>
        <item name="android:textSize">16sp</item>
        <item name="android:text">Cancel</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:layout_marginLeft">10dp</item>
    </style>

    <style name="styleOkCancelButtonLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/colorSoftGray</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="styleOkCancelButtonDivider">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/colorGray</item>
    </style>

    <style name="styleSetupModuleButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/button_gray</item>
        <item name="android:textColor">@color/colorBlack</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="styleSetupAddButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/button_red</item>
        <item name="android:textColor">@color/colorWhite</item>
        <item name="android:text">@string/add</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:textAllCaps">true</item>
    </style>

    <style name="styleSetupSearchSpinner">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="styleSetupSearchEditText">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">10dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/colorBlack</item>
        <item name="android:textColorHint">@color/colorSoftBlack</item>
        <item name="android:hint">@string/search</item>
        <item name="android:inputType">text</item>
        <item name="android:imeOptions">actionDone</item>
    </style>

    <style name="styleSetupSearchButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/button_gray</item>
        <item name="android:textColor">@color/colorBlack</item>
        <item name="android:text">@string/search</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="DividerGray">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/colorSoftGray</item>
        <item name="android:layout_margin">5dp</item>
    </style>

    <style name="styleSetupHeaderTextView">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">@color/colorSoftBlack</item>
        <item name="android:textSize">14sp</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5sp</item>
    </style>

    <style name="styleSetupListLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">50sp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">5sp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="styleSetupListTextView">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">@color/colorBlack</item>
        <item name="android:textSize">16sp</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
    </style>

    <style name="styleSetupListImageEditButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/button_gray</item>
        <item name="android:src">@drawable/ic_baseline_edit_24</item>
        <item name="android:padding">5dp</item>
    </style>

    <style name="styleSetupListImageDeleteButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/button_gray</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:src">@drawable/ic_baseline_close_24</item>
        <item name="android:padding">5dp</item>
    </style>

    <style name="styleSetupDialogSpinner">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <!--  <item name="android:background">@color/colorGray1</item>
          <item name="android:popupBackground">@color/colorGray2</item>-->
    </style>

    <style name="styleSetupDialogEditText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">10dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/colorBlack</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:textColorHint">@color/colorSoftGray</item>
        <item name="android:inputType">text</item>
    </style>

    <style name="dialogBottom">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:gravity">right</item>
    </style>

    <style name="styleSetupDialogSaveButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">10dp</item>
        <item name="android:background">@drawable/button_gray</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">16sp</item>
        <item name="android:text">@string/save</item>
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:layout_marginLeft">5dp</item>
    </style>

    <style name="styleSetupDialogCloseButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">10dp</item>
        <item name="android:background">@drawable/button_gray</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">16sp</item>
        <item name="android:text">@string/close</item>
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="dialogSubTitle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:padding">10dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/colorPrimaryDark</item>
    </style>

    <style name="SaleSubItemPrice">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:textSize">15sp</item>
        <item name="android:gravity">right</item>
    </style>

    <style name="SaleSubItemName">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">2</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="dialogClose">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/button_transparent</item>
        <item name="android:layout_gravity">right</item>
        <item name="android:src">@drawable/ic_baseline_close_24</item>
        <item name="android:tint">@color/colorPrimary</item>
    </style>

    <style name="SaleSubTitle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/colorBlack</item>
        <item name="android:textSize">16sp</item>
    </style>

</resources>
